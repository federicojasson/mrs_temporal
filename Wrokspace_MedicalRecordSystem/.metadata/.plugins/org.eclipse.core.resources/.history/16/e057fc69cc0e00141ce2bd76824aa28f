package entities;

public enum BloodType {
	
	A_POSITIVE("A+"),
	A_NEGATIVE("A-"),
	AB_POSITIVE("AB+"),
	AB_NEGATIVE("AB-"),
	B_POSITIVE("B+"),
	B_NEGATIVE("B-"),
	O_POSITIVE("O+"),
	O_NEGATIVE("O-");
	
	private String label;
	
	private BloodType(String label) {
		this.label = label;
	}
	
	public String toString() {
		return label;
	}
	
}
