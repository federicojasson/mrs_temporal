package entities;

public enum BloodType {

	O_NEGATIVE((byte) '0', "O-"),
	O_POSITIVE((byte) '0', "O+"),
	A_NEGATIVE((byte) '0', "A-"),
	A_POSITIVE((byte) '0', "A+"),
	B_NEGATIVE((byte) '0', "B-"),
	B_POSITIVE((byte) '0', "B+"),
	AB_NEGATIVE((byte) '0', "AB-"),
	AB_POSITIVE((byte) '0', "AB+");

	private String label;
	private byte value;

	private BloodType(String label, byte value) {
		this.label = label;
		this.value = value;
	}

	public String toString() {
		return label;
	}

}
