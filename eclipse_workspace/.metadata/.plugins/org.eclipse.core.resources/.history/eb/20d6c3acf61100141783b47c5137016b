package entities;

public enum BloodType {

	O_NEGATIVE('0', "O-"),
	O_POSITIVE("O+"),
	A_NEGATIVE("A-"),
	A_POSITIVE("A+"),
	B_NEGATIVE("B-"),
	B_POSITIVE("B+"),
	AB_NEGATIVE("AB-"),
	AB_POSITIVE("AB+");

	private String label;
	private byte value;

	private BloodType(String label, byte value) {
		this.label = label;
		this.value = value;
	}

	public String toString() {
		return label;
	}

}
