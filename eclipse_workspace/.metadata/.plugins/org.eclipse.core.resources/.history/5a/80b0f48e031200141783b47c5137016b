package entities;

public class AuthenticationData {
	
	private byte[] password;
	private byte[] salt;
	
	public AuthenticationData(byte[] password, byte[] salt) {
		this.password = password;
		this.salt = salt;
	}
	
	public byte[] getPassword() {
		return password;
	}
	
	public byte[] getSalt() {
		return salt;
	}
	
}
