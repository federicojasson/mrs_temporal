package workers;

import javax.swing.SwingWorker;
import modules.UserManager;

public class LoginUserDoctorWorker extends SwingWorker<Void, Void> {
	
	private String id;
	private byte[] password;
	
	public LoginUserDoctorWorker(String id, byte[] password) {
		this.id = id;
		this.password = password;
	}
	
	protected Void doInBackground() {
		// TODO
		
		UserManager.loginUserDoctor(id, password);
		
		return null;
	}
	
}
