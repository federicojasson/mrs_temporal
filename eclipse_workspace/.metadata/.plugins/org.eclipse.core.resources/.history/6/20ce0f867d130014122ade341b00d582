package gui;

import javax.swing.JFrame;
import modules.ApplicationManager;

public class ErrorFrame extends JFrame {
	
	public ErrorFrame(Exception exception) {
		super();
	}
	
	public ErrorFrame(String message) {
		super();
	}
	
	public ErrorFrame(String message, Exception exception) {
		super();
	}
	
	private void onClose() {
		ApplicationManager.exitAbnormally(exception);
	}
	
}
